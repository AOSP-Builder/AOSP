#!/bin/bash
echo "$rclone_config" > ~/.config/rclone/rclone.conf

cd /tmp
com() 
{ 
    tar --use-compress-program="pigz -k -$2 " -cf $1.tar.gz $1
}
time com ccache 1
time rclone copy -v ccache.tar.gz u:Share/OUT/CherishOS/


cd /tmp/rom
tar cf - out | pigz -1 -p 30 > out.tar.gz
mkdir -p ~/.config/rclone
time rclone copy -v out.tar.gz u:Share/OUT/CherishOS/

# cd /tmp/rom/out/target/product/whyred

# time rclone copy -v lineage*whyred.zip d:
